
<Piechart>
    label:label


    on_leave: root.remove_chart()


    MDBoxLayout:
        orientation: "vertical"

        MDToolbar:
            md_bg_color:0,0,0,0
            elevation:0
            left_action_items: [["arrow-left", lambda x: root.parent.parent.change_screen('screen1')]]



        MDBoxLayout:
            id: chart_box
            adaptive_height: True
            padding: "24dp"
            orientation: "vertical"

        MDLabel:
            id:label
            text:""
            font_style:'H6'
            halign:"center"





